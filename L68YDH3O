<script>//<![CDATA[
        let currentStep = 1;
        const totalSteps = 3;

        // Navigation functions
        function showSection(sectionId) {
            // Hide all sections
            document.querySelectorAll('.section-content').forEach(section => {
                section.classList.add('hidden');
            });
            
            // Show selected section
            document.getElementById(sectionId).classList.remove('hidden');
            
            // Update navigation
            document.querySelectorAll('.nav-btn').forEach(btn => {
                btn.classList.remove('bg-white', 'bg-opacity-20');
            });
            event.target.classList.add('bg-white', 'bg-opacity-20');
        }

        // Form step navigation
        function changeStep(direction) {
            if (direction === 1 && currentStep < totalSteps) {
                if (validateCurrentStep()) {
                    currentStep++;
                    updateStepDisplay();
                }
            } else if (direction === -1 && currentStep > 1) {
                currentStep--;
                updateStepDisplay();
            }
        }

        function updateStepDisplay() {
            // Hide all steps
            document.querySelectorAll('.form-step').forEach(step => {
                step.classList.add('hidden');
            });
            
            // Show current step
            document.getElementById(`step${currentStep}`).classList.remove('hidden');
            
            // Update step indicators
            document.querySelectorAll('.step-indicator').forEach((indicator, index) => {
                if (index + 1 <= currentStep) {
                    indicator.classList.add('active');
                } else {
                    indicator.classList.remove('active');
                }
            });
            
            // Update buttons
            const prevBtn = document.getElementById('prevBtn');
            const nextBtn = document.getElementById('nextBtn');
            const submitBtn = document.getElementById('submitBtn');
            
            prevBtn.classList.toggle('hidden', currentStep === 1);
            nextBtn.classList.toggle('hidden', currentStep === totalSteps);
            submitBtn.classList.toggle('hidden', currentStep !== totalSteps);
            
            // Update confirmation data on step 3
            if (currentStep === 3) {
                updateConfirmationData();
            }
        }

        function validateCurrentStep() {
            const currentStepElement = document.getElementById(`step${currentStep}`);
            const requiredFields = currentStepElement.querySelectorAll('[required]');
            
            for (let field of requiredFields) {
                if (!field.value.trim()) {
                    field.focus();
                    alert('Mohon lengkapi semua field yang wajib diisi');
                    return false;
                }
            }
            return true;
        }

        function updateConfirmationData() {
            const data = {
                'Nama Lengkap': document.getElementById('namaLengkap').value,
                'NISN': document.getElementById('nisn').value,
                'Tempat, Tanggal Lahir': `${document.getElementById('tempatLahir').value}, ${document.getElementById('tanggalLahir').value}`,
                'Jenis Kelamin': document.getElementById('jenisKelamin').value,
                'Agama': document.getElementById('agama').value,
                'Alamat': document.getElementById('alamat').value,
                'No. Telepon': document.getElementById('telepon').value,
                'Email': document.getElementById('email').value || '-',
                'Sekolah Asal': document.getElementById('sekolahAsal').value,
                'Jalur Pendaftaran': document.getElementById('jalurPendaftaran').value,
                'Rata-rata Nilai': document.getElementById('nilaiRapor').value,
                'Nama Ayah': document.getElementById('namaAyah').value,
                'Nama Ibu': document.getElementById('namaIbu').value
            };
            
            let html = '<h4 class="font-semibold mb-4">Data yang akan didaftarkan:</h4>';
            for (let [key, value] of Object.entries(data)) {
                html += `<div class="flex justify-between py-2 border-b border-gray-200">
                    <span class="font-medium">${key}:</span>
                    <span>${value}</span>
                </div>`;
            }
            
            document.getElementById('confirmationData').innerHTML = html;
        }

        // Global variable to store form data
        let currentFormData = null;

        // Form submission
        document.getElementById('registrationForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            if (!document.getElementById('agreement').checked) {
                alert('Mohon centang persetujuan terlebih dahulu');
                return;
            }
            
            // Generate registration number
            const registrationNumber = 'PPDB' + Date.now().toString().slice(-8);
            
            // Collect all form data
            currentFormData = {
                number: registrationNumber,
                namaLengkap: document.getElementById('namaLengkap').value,
                nisn: document.getElementById('nisn').value,
                tempatLahir: document.getElementById('tempatLahir').value,
                tanggalLahir: document.getElementById('tanggalLahir').value,
                jenisKelamin: document.getElementById('jenisKelamin').value,
                agama: document.getElementById('agama').value,
                alamat: document.getElementById('alamat').value,
                telepon: document.getElementById('telepon').value,
                email: document.getElementById('email').value,
                sekolahAsal: document.getElementById('sekolahAsal').value,
                npsnAsal: document.getElementById('npsnAsal').value,
                jalurPendaftaran: document.getElementById('jalurPendaftaran').value,
                nilaiRapor: document.getElementById('nilaiRapor').value,
                namaAyah: document.getElementById('namaAyah').value,
                namaIbu: document.getElementById('namaIbu').value,
                pekerjaanAyah: document.getElementById('pekerjaanAyah').value,
                pekerjaanIbu: document.getElementById('pekerjaanIbu').value,
                submissionDate: new Date().toLocaleDateString('id-ID'),
                submissionTime: new Date().toLocaleTimeString('id-ID'),
                status: 'Menunggu Verifikasi'
            };
            
            // Show submission method modal
            document.getElementById('submissionModal').classList.remove('hidden');
        });

        // Submission method functions
        function submitViaWhatsApp() {
            if (!currentFormData) return;
            
            const whatsappMessage = formatDataForWhatsApp(currentFormData);
            const whatsappUrl = `https://wa.me/6285117136998?text=${encodeURIComponent(whatsappMessage)}`;
            
            // Store registration data locally
            storeRegistrationData(currentFormData);
            
            // Update success modal
            document.getElementById('registrationNumber').textContent = currentFormData.number;
            document.getElementById('submissionMethod').textContent = 'WhatsApp';
            
            // Close submission modal and show success modal
            document.getElementById('submissionModal').classList.add('hidden');
            document.getElementById('successModal').classList.remove('hidden');
            
            // Open WhatsApp
            setTimeout(() => {
                window.open(whatsappUrl, '_blank', 'noopener,noreferrer');
            }, 500);
        }

        function submitViaEmail() {
            if (!currentFormData) return;
            
            const emailSubject = `PPDB Baru - ${currentFormData.namaLengkap} (${currentFormData.number})`;
            const emailBody = formatDataForEmail(currentFormData);
            const emailUrl = `mailto:aanfauzi622@gmail.com?subject=${encodeURIComponent(emailSubject)}&body=${encodeURIComponent(emailBody)}`;
            
            // Store registration data locally
            storeRegistrationData(currentFormData);
            
            // Update success modal
            document.getElementById('registrationNumber').textContent = currentFormData.number;
            document.getElementById('submissionMethod').textContent = 'Email';
            
            // Close submission modal and show success modal
            document.getElementById('submissionModal').classList.add('hidden');
            document.getElementById('successModal').classList.remove('hidden');
            
            // Open email client
            setTimeout(() => {
                window.open(emailUrl, '_blank', 'noopener,noreferrer');
            }, 500);
        }

        function submitViaBoth() {
            if (!currentFormData) return;
            
            const emailSubject = `PPDB Baru - ${currentFormData.namaLengkap} (${currentFormData.number})`;
            const emailBody = formatDataForEmail(currentFormData);
            const emailUrl = `mailto:aanfauzi622@gmail.com?subject=${encodeURIComponent(emailSubject)}&body=${encodeURIComponent(emailBody)}`;
            
            const whatsappMessage = formatDataForWhatsApp(currentFormData);
            const whatsappUrl = `https://wa.me/6285117136998?text=${encodeURIComponent(whatsappMessage)}`;
            
            // Store registration data locally
            storeRegistrationData(currentFormData);
            
            // Update success modal
            document.getElementById('registrationNumber').textContent = currentFormData.number;
            document.getElementById('submissionMethod').textContent = 'WhatsApp & Email';
            
            // Close submission modal and show success modal
            document.getElementById('submissionModal').classList.add('hidden');
            document.getElementById('successModal').classList.remove('hidden');
            
            // Open both with delays
            setTimeout(() => {
                window.open(emailUrl, '_blank', 'noopener,noreferrer');
            }, 500);
            
            setTimeout(() => {
                window.open(whatsappUrl, '_blank', 'noopener,noreferrer');
            }, 1500);
        }

        function closeSubmissionModal() {
            document.getElementById('submissionModal').classList.add('hidden');
            currentFormData = null;
        }

        function storeRegistrationData(formData) {
            const registrationData = {
                number: formData.number,
                name: formData.namaLengkap,
                nisn: formData.nisn,
                birthDate: formData.tanggalLahir,
                status: 'Menunggu Verifikasi',
                submissionDate: formData.submissionDate
            };
            
            localStorage.setItem('ppdbRegistration', JSON.stringify(registrationData));
        }

        function formatDataForEmail(data) {
            return `
PENDAFTARAN PPDB BARU
======================

INFORMASI PENDAFTARAN:
- Nomor Pendaftaran: ${data.number}
- Tanggal Daftar: ${data.submissionDate} ${data.submissionTime}
- Status: ${data.status}

DATA PRIBADI SISWA:
- Nama Lengkap: ${data.namaLengkap}
- NISN: ${data.nisn}
- Tempat, Tanggal Lahir: ${data.tempatLahir}, ${data.tanggalLahir}
- Jenis Kelamin: ${data.jenisKelamin}
- Agama: ${data.agama}
- Alamat: ${data.alamat}
- No. Telepon: ${data.telepon}
- Email: ${data.email || '-'}

DATA SEKOLAH:
- Sekolah Asal: ${data.sekolahAsal}
- NPSN Sekolah Asal: ${data.npsnAsal}
- Jalur Pendaftaran: ${data.jalurPendaftaran}
- Rata-rata Nilai Rapor: ${data.nilaiRapor}

DATA ORANG TUA:
- Nama Ayah: ${data.namaAyah}
- Pekerjaan Ayah: ${data.pekerjaanAyah}
- Nama Ibu: ${data.namaIbu}
- Pekerjaan Ibu: ${data.pekerjaanIbu}

---
Dikirim otomatis dari Sistem PPDB Online SMA Taman Siswa Genteng Banyuwangi
            `.trim();
        }

        function formatDataForWhatsApp(data) {
            return `
🏫 *PENDAFTARAN PPDB BARU*

📋 *INFORMASI PENDAFTARAN:*
• Nomor: ${data.number}
• Tanggal: ${data.submissionDate} ${data.submissionTime}
• Status: ${data.status}

👤 *DATA SISWA:*
• Nama: ${data.namaLengkap}
• NISN: ${data.nisn}
• TTL: ${data.tempatLahir}, ${data.tanggalLahir}
• JK: ${data.jenisKelamin}
• Agama: ${data.agama}
• Alamat: ${data.alamat}
• HP: ${data.telepon}
• Email: ${data.email || '-'}

🎓 *DATA SEKOLAH:*
• Asal: ${data.sekolahAsal}
• NPSN: ${data.npsnAsal}
• Jalur: ${data.jalurPendaftaran}
• Nilai: ${data.nilaiRapor}

👨‍👩‍👧‍👦 *ORANG TUA:*
• Ayah: ${data.namaAyah} (${data.pekerjaanAyah})
• Ibu: ${data.namaIbu} (${data.pekerjaanIbu})

_Dikirim otomatis dari Sistem PPDB Online_
            `.trim();
        }

        // Status check
        document.getElementById('statusForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            const nisn = document.getElementById('statusNisn').value;
            const birthDate = document.getElementById('statusTanggalLahir').value;
            
            // Check stored registration data
            const storedData = localStorage.getItem('ppdbRegistration');
            
            if (storedData) {
                const data = JSON.parse(storedData);
                if (data.nisn === nisn && data.birthDate === birthDate) {
                    showStatusResult(data);
                } else {
                    showStatusNotFound();
                }
            } else {
                showStatusNotFound();
            }
        });

        function showStatusResult(data) {
            const statusColors = {
                'Menunggu Verifikasi': 'bg-yellow-100 text-yellow-800',
                'Diterima': 'bg-green-100 text-green-800',
                'Ditolak': 'bg-red-100 text-red-800'
            };
            
            const html = `
                <div class="text-center">
                    <h4 class="text-lg font-semibold mb-4">Status Pendaftaran Ditemukan</h4>
                    <div class="space-y-3">
                        <div class="flex justify-between">
                            <span class="font-medium">Nomor Pendaftaran:</span>
                            <span class="font-bold text-blue-600">${data.number}</span>
                        </div>
                        <div class="flex justify-between">
                            <span class="font-medium">Nama:</span>
                            <span>${data.name}</span>
                        </div>
                        <div class="flex justify-between">
                            <span class="font-medium">Tanggal Daftar:</span>
                            <span>${data.submissionDate}</span>
                        </div>
                        <div class="flex justify-between items-center">
                            <span class="font-medium">Status:</span>
                            <span class="px-3 py-1 rounded-full text-sm font-medium ${statusColors[data.status] || 'bg-gray-100 text-gray-800'}">${data.status}</span>
                        </div>
                    </div>
                    <div class="mt-6 p-4 bg-blue-50 rounded-lg">
                        <p class="text-sm text-blue-700">
                            ${data.status === 'Menunggu Verifikasi' ? 
                                'Pendaftaran Anda sedang dalam proses verifikasi. Hasil akan diumumkan pada 25 Juni 2026.' :
                                data.status === 'Diterima' ?
                                'Selamat! Anda diterima di SMA Taman Siswa Genteng Banyuwangi. Silakan lakukan daftar ulang.' :
                                'Mohon maaf, pendaftaran Anda tidak dapat diterima. Silakan coba jalur lain.'
                            }
                        </p>
                    </div>
                    <div class="mt-6 flex space-x-3">
                        <button onclick="printStatus()" class="flex-1 bg-green-600 text-white py-2 px-4 rounded-lg hover:bg-green-700 transition-colors">
                            <i class="ri-printer-line mr-2"></i>Cetak Bukti
                        </button>
                        <button onclick="downloadStatusPDF('${data.number}')" class="flex-1 bg-emerald-600 text-white py-2 px-4 rounded-lg hover:bg-emerald-700 transition-colors">
                            <i class="ri-file-pdf-line mr-2"></i>Download PDF
                        </button>
                        <button onclick="shareStatus('${data.number}')" class="flex-1 bg-teal-600 text-white py-2 px-4 rounded-lg hover:bg-teal-700 transition-colors">
                            <i class="ri-share-line mr-2"></i>Bagikan
                        </button>
                    </div>
                </div>
            `;
            
            document.getElementById('statusResult').innerHTML = html;
            document.getElementById('statusResult').classList.remove('hidden');
        }

        function showStatusNotFound() {
            const html = `
                <div class="text-center">
                    <div class="w-16 h-16 bg-red-100 rounded-full flex items-center justify-center mx-auto mb-4">
                        <i class="ri-close-circle-line text-3xl text-red-600"></i>
                    </div>
                    <h4 class="text-lg font-semibold mb-2">Data Tidak Ditemukan</h4>
                    <p class="text-gray-600">NISN atau tanggal lahir yang Anda masukkan tidak sesuai dengan data pendaftaran. Pastikan data yang dimasukkan benar.</p>
                </div>
            `;
            
            document.getElementById('statusResult').innerHTML = html;
            document.getElementById('statusResult').classList.remove('hidden');
        }

        function closeModal() {
            document.getElementById('successModal').classList.add('hidden');
            // Reset form and go back to home
            document.getElementById('registrationForm').reset();
            currentStep = 1;
            updateStepDisplay();
            showSection('home');
        }

        // Print status function
        function printStatus() {
            const printContent = document.getElementById('statusResult').innerHTML;
            const printWindow = window.open('', '_blank');
            printWindow.document.write(`
                <html>
                <head>
                    <title>Bukti Status PPDB</title>
                    <style>
                        body { font-family: Arial, sans-serif; padding: 20px; }
                        .header { text-align: center; margin-bottom: 30px; border-bottom: 2px solid #333; padding-bottom: 20px; }
                        .content { margin: 20px 0; }
                        .footer { margin-top: 50px; text-align: center; font-size: 12px; color: #666; }
                        @media print { .no-print { display: none; } }
                    </style>
                </head>
                <body>
                    <div class="header">
                        <h1>SMA TAMAN SISWA GENTENG BANYUWANGI</h1>
                        <h2>BUKTI STATUS PENDAFTARAN PPDB</h2>
                        <p>Tahun ajaran 2026/2027</p>
                    </div>
                    <div class="content">
                        ${printContent}
                    </div>
                    <div class="footer">
                        <p>Dicetak pada: ${new Date().toLocaleString('id-ID')}</p>
                        <p>Dokumen ini sah tanpa tanda tangan basah</p>
                    </div>
                </body>
                </html>
            `);
            printWindow.document.close();
            printWindow.print();
        }

        // Download PDF function
        function downloadStatusPDF(registrationNumber) {
            const storedData = localStorage.getItem('ppdbRegistration');
            if (!storedData) return;
            
            const data = JSON.parse(storedData);
            const pdfContent = `
BUKTI STATUS PENDAFTARAN PPDB
SMA TAMAN SISWA GENTENG BANYUWANGI
Tahun ajaran 2026/2027

===============================

Nomor Pendaftaran: ${data.number}
Nama Siswa: ${data.name}
NISN: ${data.nisn}
Tanggal Lahir: ${data.birthDate}
Status: ${data.status}
Tanggal Daftar: ${data.submissionDate}

===============================

Catatan:
${data.status === 'Menunggu Verifikasi' ? 
    'Pendaftaran sedang dalam proses verifikasi. Hasil akan diumumkan pada 25 Juni 2026.' :
    data.status === 'Diterima' ?
    'Selamat! Anda diterima di SMA Taman Siswa Genteng Banyuwangi. Silakan lakukan daftar ulang.' :
    'Mohon maaf, pendaftaran tidak dapat diterima. Silakan coba jalur lain.'
}

Dicetak pada: ${new Date().toLocaleString('id-ID')}
Dokumen ini sah tanpa tanda tangan basah

---
Sistem PPDB Online SMA Taman Siswa Genteng Banyuwangi
            `;
            
            // Create and download text file (as PDF generation requires external library)
            const blob = new Blob([pdfContent], { type: 'text/plain' });
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = `Bukti_PPDB_${registrationNumber}.txt`;
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            window.URL.revokeObjectURL(url);
            
            alert('File bukti berhasil didownload! Anda dapat mencetak atau menyimpannya sebagai backup.');
        }

        // Share status function
        function shareStatus(registrationNumber) {
            const storedData = localStorage.getItem('ppdbRegistration');
            if (!storedData) return;
            
            const data = JSON.parse(storedData);
            const shareText = `
🏫 BUKTI PENDAFTARAN PPDB
SMA Taman Siswa Genteng Banyuwangi

📋 Nomor: ${data.number}
👤 Nama: ${data.name}
🆔 NISN: ${data.nisn}
📅 Tanggal Daftar: ${data.submissionDate}
📊 Status: ${data.status}

${data.status === 'Menunggu Verifikasi' ? 
    '⏳ Sedang dalam proses verifikasi' :
    data.status === 'Diterima' ?
    '✅ DITERIMA - Silakan daftar ulang' :
    '❌ Tidak diterima'
}

#PPDB2026 #SMATamanSiswaGentengBanyuwangi
            `;
            
            if (navigator.share) {
                navigator.share({
                    title: 'Bukti Pendaftaran PPDB',
                    text: shareText,
                    url: window.location.href
                }).catch(err => console.log('Share cancelled'));
            } else {
                // Fallback: copy to clipboard
                navigator.clipboard.writeText(shareText).then(() => {
                    alert('Teks bukti berhasil disalin ke clipboard! Anda dapat membagikannya di media sosial atau aplikasi chat.');
                }).catch(() => {
                    // Fallback for older browsers
                    const textArea = document.createElement('textarea');
                    textArea.value = shareText;
                    document.body.appendChild(textArea);
                    textArea.select();
                    document.execCommand('copy');
                    document.body.removeChild(textArea);
                    alert('Teks bukti berhasil disalin! Anda dapat membagikannya di media sosial atau aplikasi chat.');
                });
            }
        }

        // Scroll to top functionality
        function initScrollToTop() {
            const scrollToTopBtn = document.getElementById('scrollToTop');
            
            window.addEventListener('scroll', function() {
                if (window.pageYOffset > 300) {
                    scrollToTopBtn.classList.remove('hidden');
                } else {
                    scrollToTopBtn.classList.add('hidden');
                }
            });
            
            scrollToTopBtn.addEventListener('click', function() {
                window.scrollTo({
                    top: 0,
                    behavior: 'smooth'
                });
            });
        }

        // Mobile menu functions
        function toggleMobileMenu() {
            const overlay = document.getElementById('mobileMenuOverlay');
            const panel = document.getElementById('mobileMenuPanel');
            const hamburger = document.querySelector('.hamburger');
            
            if (overlay.classList.contains('hidden')) {
                // Open menu
                overlay.classList.remove('hidden');
                setTimeout(() => {
                    panel.classList.add('mobile-menu-open');
                }, 10);
                hamburger.classList.add('active');
                document.body.style.overflow = 'hidden';
            } else {
                // Close menu
                closeMobileMenu();
            }
        }
        
        function closeMobileMenu() {
            const overlay = document.getElementById('mobileMenuOverlay');
            const panel = document.getElementById('mobileMenuPanel');
            const hamburger = document.querySelector('.hamburger');
            
            panel.classList.remove('mobile-menu-open');
            hamburger.classList.remove('active');
            document.body.style.overflow = '';
            
            setTimeout(() => {
                overlay.classList.add('hidden');
            }, 300);
        }
        
        // Initialize mobile menu event listeners
        function initMobileMenu() {
            const mobileMenuBtn = document.getElementById('mobileMenuBtn');
            const closeMobileMenuBtn = document.getElementById('closeMobileMenu');
            const overlay = document.getElementById('mobileMenuOverlay');
            
            // Toggle menu on button click
            mobileMenuBtn.addEventListener('click', toggleMobileMenu);
            
            // Close menu on close button click
            closeMobileMenuBtn.addEventListener('click', closeMobileMenu);
            
            // Close menu when clicking overlay
            overlay.addEventListener('click', function(e) {
                if (e.target === overlay) {
                    closeMobileMenu();
                }
            });
            
            // Close menu on escape key
            document.addEventListener('keydown', function(e) {
                if (e.key === 'Escape' && !overlay.classList.contains('hidden')) {
                    closeMobileMenu();
                }
            });
            
            // Handle window resize
            window.addEventListener('resize', function() {
                if (window.innerWidth >= 768 && !overlay.classList.contains('hidden')) {
                    closeMobileMenu();
                }
            });
        }

        // Download App Functions
        function showDownloadModal() {
            document.getElementById('downloadAppModal').classList.remove('hidden');
        }

        function closeDownloadModal() {
            document.getElementById('downloadAppModal').classList.add('hidden');
        }

        function downloadAndroidApp() {
            // Create a simulated APK download
            const appData = generateAppData();
            const blob = new Blob([appData], { type: 'application/vnd.android.package-archive' });
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = 'https://cdn.jsdelivr.net/gh/anzi-developer/master@main/PPDB%20SMA%20TAMAN%20SISWA%20GENTENG%20V.1.0.apk';
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            window.URL.revokeObjectURL(url);
            
            showDownloadSuccess('Android');
        }

        function downloadIOSApp() {
            // Create a simulated iOS app package
            const appData = generateAppData();
            const blob = new Blob([appData], { type: 'application/octet-stream' });
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = 'PPDB_SMA_Taman_Siswa_Genteng.ipa';
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            window.URL.revokeObjectURL(url);
            
            showDownloadSuccess('iOS');
        }

        function downloadWebApp() {
            // Install as PWA or create desktop shortcut
            if ('serviceWorker' in navigator) {
                // Register service worker for PWA functionality
                navigator.serviceWorker.register('/sw.js').catch(() => {
                    // Fallback: create desktop shortcut file
                    createDesktopShortcut();
                });
            } else {
                createDesktopShortcut();
            }
            
            showDownloadSuccess('Web App (PWA)');
        }

        function createDesktopShortcut() {
            const shortcutContent = `[Desktop Entry]
Version=1.0
Type=Application
Name=PPDB SMA Taman Siswa Genteng
Comment=Sistem Penerimaan Peserta Didik Baru
Exec=xdg-open "${window.location.href}"
Icon=data:image/svg+xml;base64,
Terminal=false
Categories=Education;Office;`;
            
            const blob = new Blob([shortcutContent], { type: 'application/x-desktop' });
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = "https://ppdbsmatsg.blogspot.com/?m=1";
            a.download = 'PPDB_SMA_Taman_Siswa_Genteng.shortcut';
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            window.URL.revokeObjectURL(url);
        }

        function generateAppData() {
            // Generate a comprehensive app package with all current functionality
            const appContent = `
PPDB SMA TAMAN SISWA GENTENG BANYUWANGI
